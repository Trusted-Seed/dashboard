{
  "name": "ts-dashboard",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "typecheck": "tsc",
    "lint:nofix": "eslint --ignore-path .gitignore \"*/**/*.{js,jsx,ts,tsx}\"",
    "lint": "yarn lint:nofix --quiet --fix",
    "format": "prettier --ignore-path .gitignore --write \"{*,**/*}.{ts,tsx,js,jsx,json,yml,yaml,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-react": "^7.16.7",
    "@next/eslint-plugin-next": "^12.1.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "frontmatter-markdown-loader": "^3.6.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "netlify-cms-proxy-server": "^1.3.23",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.6",
    "@davatar/react": "^1.9.0",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@gnosis.pm/safe-apps-web3modal": "^10.0.0",
    "ethers": "^5.5.4",
    "fast-memoize": "^2.5.2",
    "framer-motion": "^6.2.8",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.0",
    "typescript": "^4.6.2",
    "web3modal": "^1.9.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
